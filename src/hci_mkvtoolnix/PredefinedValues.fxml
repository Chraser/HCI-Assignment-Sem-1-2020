<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root id="AnchorPane" prefHeight="646.0" prefWidth="1090.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="categoryList" layoutX="48.0" layoutY="16.0" onAction="#changeSelection" prefHeight="33.0" prefWidth="338.0" />
      <ListView fx:id="listView" layoutX="48.0" layoutY="64.0" prefHeight="559.0" prefWidth="887.0" />
      <Button layoutX="987.0" layoutY="100.0" mnemonicParsing="false" onAction="#handleAddAction">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/icons/plus-icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="987.0" layoutY="164.0" mnemonicParsing="false" onAction="#handleRemoveAction">
         <graphic>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resources/icons/minus-icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
